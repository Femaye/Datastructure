let arr1 = [1, 2, 3, 4, 5,];
let arr2 = [5, 6, 7, 8, 9,];

let a = arr1.length;
let b = arr2.length;

let sum = 0;

function addToSum (arr1, arr2, a, b) {
    let allElement = new Set();

    for(i = 0;  i < b; i++) {
        allElement.add(b[i]);
        
    for(i = 0; i < a; i++){
        if(!allElement.has(a[i]))
            console.log(a[i] + sum);
        
        }
    sum += i;
    }
    return sum
}